#include "constant.h"

#ifndef CALIB
#include "state.nxc"
#include "calibration.nxc"
#endif

#include "movement.nxc"
#include "arm.nxc"

task main() {
    SetSensorTouch(S1);
    SetSensorLowspeed(S2);
    SetSensorUltrasonic(S3);
    SetSensorLight(S4);
#ifdef CALIB
    calibrate_arm();
    horiz();
    rotate(10);
#else
    init_pos();
    init_arm();
    init_calibrate();
    mainLoop();
#endif
}
